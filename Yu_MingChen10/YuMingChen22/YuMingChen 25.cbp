#include<iostream>

using namespace std;

int qualityPoint(int n)
{
    if (n<=100&&n>=90)
        return 4;
    if (n<=89&&n>=80)
        return 3;
    if (n<=79&&n>=70)
        return 2;
    if (n<=69&&n>=60)
        return 1;
    if (n<=59)
        return 0;
}
int main()
{
    int i;
    cin>>i;
    cout<<qualityPoint(i);
}
